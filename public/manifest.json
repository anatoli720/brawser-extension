{
  "manifest_version": 3,
  "version": "0.0.1",
  "name": "Курс криптовалют",
  "description":"Выводим через popap курс пары токенов",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "scripting"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "32":"./bitcoin32.png",
    "64":"./bitcoin64.png",
    "128":"./bitcoin128.png"
  }
}